{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "label": {
      "type": "string"
    },
    "key": {
      "type": "string"
    },
    "type": {
      "description": "Buckets have a type. The bucket type can indicate that applications in it are 'done', i.e. at the end of the process, and whether there was a hire, or an exit.",
      "enum": [
        "inProgress",
        "doneFilled",
        "doneUnFilled",
        "doneExFilled"
      ]
    }
  },
  "required": [
    "key",
    "label",
    "type"
  ],
  "additionalProperties": false
}