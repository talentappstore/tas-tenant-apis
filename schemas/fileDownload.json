{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "fileName": {
            "$ref": "fileUpload.json#/definitions/fileName"
        },
        "via": {
            "$ref": "fileUpload.json#/definitions/via"
        },
        "size": {
            "type": "integer"
        },
        "md5": {
            "type": "string",
            "description": "hash of the file's binary content"
        },
        "dateUploaded": {
            "type": "string",
            "format": "date-time",
            "description": "the date and time the file was uploaded"
        },
        "link": {
            "type": "string",
            "format": "uri",
            "description": "expiring link to access the raw data"
        }
    },
    "additionalProperties": false,
    "required": [
        "link"
    ]
}

