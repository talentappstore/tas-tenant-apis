{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "testFlag": {
            "type": "boolean",
            "description": "true if this set is to be applied as a dry run - i.e. no changes should be made to the jobs at the target"
        },
        "jobs": {
            "type": "array",
            "items": {
                "$ref": "jobSetJob.json"
            }
        }
    },
    "required": [
        "testFlag",
        "jobs"
    ],
    "additionalProperties": false
}