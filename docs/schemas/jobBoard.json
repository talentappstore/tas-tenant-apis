{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "immutable id for this job board"
    },
    "label": {
      "type": "string",
      "description": "label for this job board, e.g. Super grads"
    }
  },
  "required": [
    "id",
    "label"
  ],
  "additionalProperties": false
}